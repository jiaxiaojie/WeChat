.swiper-tab{  
    width: 100%;  
    background-color: #fff;
    border-top: 1rpx solid #f5f5f5 ; 
    border-bottom: 1rpx solid #f5f5f5 ; 
    line-height: 90rpx;
    height: 90rpx;
    }  
.swiper-tab-list{  
    font-size: 30rpx;  
    display: inline-block;  
    width: 20%;  
    color: #666;  
    text-align: center;
} 
.on{
  background: #BA8E52;
  color:#fff;
}
.item{
  width:100%;
  background-color: #fff;
  margin-top:20rpx;
}
.item .up{
  height:150rpx;
  border-bottom: 1rpx solid #f5f5f5;
  width:100%;
  padding:36rpx 30rpx 0;
  box-sizing: border-box;
}
.item .down{
  height:120rpx;
  width: 100%;
  padding:0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.up .one{
  display: flex;
  justify-content: space-between;
}
.one .title{
  color:#333;
  font-size: 30rpx;
}
.itemed.chotel{
  width:110rpx;
  height: 44rpx;
  background-color: #FFFAF4 ;
  font-size: 24rpx;
  color:#BA8E52;
  line-height: 44rpx;
  text-align: center;
}
.two {
  display: flex;
  font-size: 24rpx;
  margin-top:15rpx;
}
.down .time{
  color:#000;
  font-size: 32rpx;
}
.down .price{
  color:#E2231A;
  font-size: 32rpx;
}
.down .again{
  color:#fff;
  font-size: 26rpx;
  background: #BA8E52;
  border-radius: 20px;
  width:160rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
}
.itemcancel.chotel{
  width:110rpx;
  height: 44rpx;
  background-color: #E6E6E6 ;
  font-size: 24rpx;
  color:#666;
  line-height: 44rpx;
  text-align: center;
}
.unitem.chotel{
  width:110rpx;
  height: 44rpx;
  background-color: #E6E6E6 ;
  font-size: 24rpx;
  color:#999;
  line-height: 44rpx;
  text-align: center;
}
.ditem.chotel{
  width:110rpx;
  height: 44rpx;
  background-color: #E2231A  ;
  font-size: 24rpx;
  color:#fff;
  line-height: 44rpx;
  text-align: center;
}
.down .cancel{
  color:#BA8E52;
  font-size: 26rpx;
  border-radius: 20px;
  width:158rpx;
  height: 58rpx;
  text-align: center;
  line-height: 60rpx;
  border: 1px solid #BA8E52;
  margin-left:215rpx;
}
.emptyInfo{
  padding: 100rpx;
  text-align: center;
  font-size: 14px;
  color: #333;
}